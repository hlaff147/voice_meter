{
  "expo": {
    "name": "voice-meter-mobile",
    "slug": "voice-meter-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.voicemeter.app"
    },
    "android": {
      "package": "com.voicemeter.app"
    },
    "web": {
      "bundler": "metro"
    },
    "scheme": "voicemeter",
    "plugins": [
      "expo-router"
    ]
  }
}
